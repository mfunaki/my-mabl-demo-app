generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum ExpenseStatus {
  PENDING
  APPROVED
  REJECTED
}

model Expense {
  id        Int           @id @default(autoincrement())
  title     String
  amount    Int
  status    ExpenseStatus @default(PENDING)
  createdAt DateTime      @default(now())
}
